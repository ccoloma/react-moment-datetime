  "license": "MIT",
  "devDependencies": {
    "babel-loader": "^7.0.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "coveralls": "^2.13.1",
    "enzyme": "^2.9.1",
    "jsdom": "^11.1.0",
    "jsdom-global": "^3.0.2",
    "json-loader": "^0.5.4",
    "mocha": "^3.4.2",
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.2",
    "nyc": "^11.0.3",
    "onchange": "^3.2.1",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "sinon": "^2.3.8",
    "style-loader": "^0.17.0",
