{
  "name": "react-moment-datetime",
  "version": "0.0.1",
  "description": "React datetime picker powered by momentjs",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel dev watch:css",
    "build:css": "node-sass --output-style compressed -o test/css src/scss/datetime.scss",
    "watch:css": "onchange 'src/scss/*.scss' -- npm run build:css",
    "dev": "webpack-dev-server --inline --hot --progress --colors"
  },
  "github-pages-deploy": {
    "src": "test"
  },
  "author": "Carlos Coloma <ccescribano@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-loader": "^7.0.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "github-pages-deploy": "0.0.3",
    "json-loader": "^0.5.4",
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.2",
    "onchange": "^3.2.1",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "style-loader": "^0.17.0",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-notifier": "^1.5.0"
  },
  "dependencies": {
    "blacklist": "^1.1.4",
    "classnames": "^2.2.0",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "react-input-slider": "github:ccoloma/react-input-slider"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ccoloma/react-moment-datetime.git"
  },
  "keywords": [
    "react",
    "react-component",
    "input",
    "datetime",
    "picker",
    "moment"
  ],
  "bugs": {
    "url": "https://github.com/ccoloma/react-moment-datetime/issues"
  },
  "homepage": "https://github.com/ccoloma/react-moment-datetime#readme"
}
