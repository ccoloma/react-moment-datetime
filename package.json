    "webpack-dev-server": "^2.4.5",
    "webpack-notifier": "^1.5.0"
  },
  "dependencies": {
    "moment": "^2.17.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koliseoapi/react-moment-datetime.git"
  },
  "keywords": [
    "react",
    "react-component",
    "input",
    "datetime",
    "picker",
    "moment"
