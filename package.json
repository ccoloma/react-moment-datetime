{
  "name": "react-moment-datetime",
  "version": "0.4.0",
  "description": "React datetime picker powered by momentjs",
  "main": "index.js",
  "author": "Carlos Coloma <ccescribano@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0-beta.38",
    "babel-core": "^7.0.0-bridge.0",
    "coveralls": "^3.0.2",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "jsdom": "^13.1.0",
    "jsdom-global": "^3.0.2",
    "json-loader": "^0.5.7",
    "mocha": "^5.2.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.1.0",
    "onchange": "^5.2.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "sinon": "^7.2.2",
    "webpack": "^4.28.3",
    "webpack-dev-server": "^3.1.14",
    "webpack-notifier": "^1.7.0"
  },
  "dependencies": {
    "moment": "^2.23.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koliseoapi/react-moment-datetime.git"
  },
  "keywords": [
    "react",
    "react-component",
    "input",
    "datetime",
    "picker",
    "moment"
  ],
  "bugs": {
    "url": "https://github.com/koliseoapi/react-moment-datetime/issues"
  },
  "homepage": "https://github.com/koliseoapi/react-moment-datetime#readme",
  "scripts": {
    "watch": "npm-run-all --parallel watch:**",
    "build": "node-sass --output-style compressed -o example/css src/scss/datetime.scss",
    "watch:css": "onchange 'src/scss/*.scss' -- npm run build",
    "watch:js": "webpack-dev-server --inline --hot --progress --colors",
    "test": "./node_modules/mocha/bin/_mocha --require .vscode/registerBabel.js test/*Test.js",
    "postversion": "git push && git push --tags",
    "coverage": "NODE_ENV=test node_modules/.bin/nyc --reporter=lcov --reporter=text node_modules/.bin/_mocha --require .vscode/registerBabel.js test/*Test.js"
  }
}
